            },
            {
              "id": 2,
              "name": "Anna Schmidt",
              "abteilung": "HR",
              "position": "HR Manager"
            }
          ]
        }
      },
      "jsonB": {
        "firma": {
          "mitarbeiter": [
            {
              "id": 1,
              "name": "Max Mustermann",
              "abteilung": "IT",
              "position": "Senior Developer"
            },
            {
              "id": 2,
              "name": "Anna Schmidt",
              "abteilung": "HR",
              "position": "HR Manager"
            },
            {
              "id": 4,
